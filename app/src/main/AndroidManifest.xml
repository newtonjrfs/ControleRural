<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="controlerural.newton.com.controlerural">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityTelaPrincipal">
        </activity>
        <activity android:name=".telaPrincipal.ActivityCadastroCompra"
            android:label="Cadastro de Compras">
        </activity>
        <activity android:name=".telaCadastroUsuario.ActivityCadastroUsuario"
            android:label="Cadastro de Usuario">
        </activity>
    </application>

</manifest>